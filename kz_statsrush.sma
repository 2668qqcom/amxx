public readTops()
{
	new szDir[128];
	get_localinfo("amxx_datadir", szDir, "");
	format(szDir, "", "%s/%s", szDir, g_szTopFolder);
	if (!dir_exists(szDir))
	{
		mkdir(szDir);
	}
	new szTopDir[128];
	new szTopFile[128];
	new szData[200];
	new i;
	while (i < 9)
	{
		formatex(szTopDir, "", "%s/%s", szDir, g_szSpeedTypes[i]);
		if (!dir_exists(szTopDir))
		{
			mkdir(szTopDir);
		}
		new j;
		while (j < 16)
		{
			new k;
			while (k < 10)
			{
				g_szTopSteamID[j + i * 16][k][0] = 0;
				g_szTopName[j + i * 16][k][0] = 0;
				g_szTopDistance[j + i * 16][k][0] = 0;
				g_szTopMaxSpeed[j + i * 16][k][0] = 0;
				g_szTopPrestrafe[j + i * 16][k][0] = 0;
				g_szTopStrafes[j + i * 16][k][0] = 0;
				g_szTopEfficiency[j + i * 16][k][0] = 0;
				k++;
			}
			new var1;
			if (j == 11 || j == 12)
			{
				formatex(szTopFile, "", "%s/%s.txt", szDir, g_szTopNames[j]);
			}
			else
			{
				formatex(szTopFile, "", "%s/%s.txt", szTopDir, g_szTopNames[j]);
			}
			if (file_exists(szTopFile))
			{
				new handle = fopen(szTopFile, "rt");
				new k;
				while (k < 10 && !feof(handle))
				{
					fgets(handle, szData, 199);
					trim(szData);
					parse(szData, g_szTopSteamID[j + i * 16][k], 31, g_szTopName[j + i * 16][k], 31, g_szTopDistance[j + i * 16][k], 9, g_szTopMaxSpeed[j + i * 16][k], 9, g_szTopPrestrafe[j + i * 16][k], 9, g_szTopStrafes[j + i * 16][k], 4, g_szTopEfficiency[j + i * 16][k], 4);
					k++;
				}
				fclose(handle);
			}
			j++;
		}
		i++;
	}
	return 0;
}

