public updateMainColor(id)
{
	if (1 < g_setMainColor[id])
	{
		g_iMainRed[id] = 255;
		g_iMainGreen[id] = 255;
		g_iMainBlue[id] = 40;
		g_iMainFailRed[id] = 255;
		g_iMainFailGreen[id] = 100;
		g_iMainFailBlue[id] = 255;
	}
	else
	{
		if (g_setMainColor[id] == 1)
		{
			g_iMainRed[id] = 30;
			g_iMainGreen[id] = 145;
			g_iMainBlue[id] = 255;
			g_iMainFailRed[id] = 255;
			g_iMainFailGreen[id] = 115;
			g_iMainFailBlue[id] = 149;
		}
		g_iMainRed[id] = get_pcvar_num(g_pcvarStatsRed);
		g_iMainGreen[id] = get_pcvar_num(g_pcvarStatsGreen);
		g_iMainBlue[id] = get_pcvar_num(g_pcvarStatsBlue);
		g_iMainFailRed[id] = get_pcvar_num(g_pcvarFailstatsRed);
		g_iMainFailGreen[id] = get_pcvar_num(g_pcvarFailstatsGreen);
		g_iMainFailBlue[id] = get_pcvar_num(g_pcvarFailstatsBlue);
	}
	return 0;
}

public saveSettings(id)
{
	new Float:fCurTime = get_gametime();
	static Float:fPrevSaveSettTime;
	if (fCurTime < floatadd(1067030938, fPrevSaveSettTime))
	{
		client_print_f(id, ColorType:5, "\x03[%s]\x01 Do not use it\x03 so frequently!", g_szPrefix);
		return 1;
	}
	fPrevSaveSettTime = fCurTime;
	static szVars[550];
	szVars[0] = 0;
	formatex(szVars, 549, "LJSTATS:%d;SHOWPRE:%d;JOF:%d;COLORCHAT:%d;SPEED:%d;BEAM:%d;", g_setStatsMode[id], g_setShowPre[id], g_setJumpOff[id], g_setColorChat[id], g_setSpeed[id], g_setBeam[id]);
	format(szVars, 549, "%sPREBIT:%d;ENABLEBIT:%d;STRALWAYS:%d;TOTALGAIN:%d;FIRSTCOL:%d;LASTCOL:%d;", szVars, g_setPre[id], g_setEnable[id], g_setStrafesAlways[id], g_setStrafesTotalGainEff[id], g_setStrafesFirstColumn[id], g_setStrafesLastColumn[id]);
	format(szVars, 549, "%sMAINCOLOR:%d;PRECOLOR:%d;JOFCOLOR:%d;LJSOUND:%d;REALGAIN:%d;FULLOVER:%d;", szVars, g_setMainColor[id], g_setPreColor[id], g_setJofColor[id], g_setLjSound[id], g_setStrafesRealGain[id], g_setStrafesFullOverlap[id]);
	format(szVars, 549, "%sSHOWMAIN:%d;CONMAIN:%d;HUDSTR:%d;CONSTR:%d;HUDPRE:%d;CONPRE:%d;", szVars, g_setMainHUD[id], g_setMainConsole[id], g_setStrafesHUD[id], g_setStrafesConsole[id], g_setPreHUD[id], g_setPreConsole[id]);
	format(szVars, 549, "%sSTYLE:%d;MAINNAME:%d;MAINMAXSPEED:%d;MAINPRE:%d;MAINEFF:%d;MAINSTR:%d;", szVars, g_setStyle[id], g_setName[id], g_setMaxSpeed[id], g_setPrestrafes[id], g_setEff[id], g_setStrafes[id]);
	new iReturn;
	ExecuteForward(g_hLjSettingsForward, iReturn, id, szVars);
	if (get_pcvar_num(g_pcvarTxtLjSettings))
	{
		new szAuthID[31];
		get_user_authid(id, szAuthID, 30);
		replace_all(szAuthID, 30, 1151280, 1151288);
		new szSettingsFile[128];
		format(szSettingsFile, "", "%s/%s.txt", g_szSettingsDir, szAuthID);
		new handle = fopen(szSettingsFile, "wt");
		fputs(handle, szVars);
		fclose(handle);
	}
	client_print_f(id, ColorType:5, "\x04[%s]\x01 LJ Settings saved", g_szPrefix);
	return 1;
}

